pipeline {
  /*agent server1/docker/kubernetes/any*/
  agent any 
  stages {
    stage('git checkout') {
      steps {
        script {
          /*type1*/
          lis1=["devops","aws","azure"]
          for (ele  in lis1) {
            println "my element is ${ele}"
          }
          for(i=1;i<=5;i++) {
            println "my i value is ${i}"
          }
          /*# while loop */
          j=6
          while (j<=10) {
            println "j value is ${j}"
            j=j+1
          }
        }
      }
    }
  } 
}
